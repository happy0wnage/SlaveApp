<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.backendless.examples.userservice.demo"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="10" android:targetSdkVersion="10"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MANAGE_DOCUMENTS"/>
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application android:label="@string/app_name" android:theme="@android:style/Theme.Holo.Light"
        tools:targetApi="honeycomb">
        <activity android:name=".MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".RegisteredActivity"/>
        <activity android:name=".ForgotActivity"/>
        <activity android:name=".BrowseActivity"/>
        <activity android:name=".FunctionalActivity"/>
        <activity android:name=".UploadingActivity"/>
        <activity android:name=".ProfileActivity"/>
        <activity android:name=".PlacesActivity"/>
        <activity android:name=".BrowsePlacesActivity"/>

        <activity
            android:name=".fileexplore.FileexplorerActivity"
            android:label="@string/title_activity_fileexplorer">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <!--<category android:name="android.intent.category.LAUNCHER" />-->
        </intent-filter>
    </activity>

    <activity
        android:name=".fileexplore.FileChooser"
        android:label="Choose File">
    <action android:name="com.example.fileexplorer.FileChooser" />
    <category android:name="android.intent.category.DEFAULT" />
</activity>

    </application>
</manifest>
